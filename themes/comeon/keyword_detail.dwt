<script type="text/javascript">
$(document).ready(function(){
    $(".tb-thumb li a").click(function(){
        var class_id = $(this).attr('rel_class');
        $("."+class_id).attr('src', $(this).find("img").attr("big"));
    });
});
</script>

<!-- return top star -->
<div class="go-top topdn" id="go-top"><a href="javascript:;" class="go"></a></div>
<script type="text/javascript">
    jQuery(function(){
        showScroll();
        function showScroll(){
            jQuery(window).scroll( function() { 
                var scrollValue=jQuery(window).scrollTop();
                scrollValue > 100 ? jQuery('.go-top').fadeIn():jQuery('.go-top').fadeOut();
            } );    
            jQuery('#go-top').click(function(){
                jQuery("html,body").animate({scrollTop:0},200);    
            });    
        }
    })
</script>
<!-- return top end -->

