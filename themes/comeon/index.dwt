            </div>
        </div>
        
        <script type="text/javascript">
            var outerlayer = $(".slide");
            var numberlayer = $(".mkeNum");
            var imgwidth = 760;
            var num = outerlayer.find("li").size();
            var cnum = 0;
            outerlayer.find("ul").width(num*imgwidth);
            
            $(".next").click(function(){
                cnum++;
                if(cnum>(num-1)){
                    cnum = 0    
                }
                outerlayer.find("ul").animate({"left":-cnum*imgwidth},300);
            });
           
           $(".pre").click(function(){
                cnum--;
                if(cnum<0){
                    cnum = num - 1;    
                }
                outerlayer.find("ul").animate({"left":-cnum*imgwidth},300);
            }); 
            
           function autoPlay(){
              cnum++;
                if(cnum>(num-1)){
                    cnum=0    
                }
                outerlayer.find("ul").animate({"left":-cnum*imgwidth},300);
            }
            var Timer = setInterval(autoPlay,4000);
            $(".slide_content").hover(function(){clearInterval(Timer)},function(){Timer=setInterval(autoPlay,4000);}); 
        </script>
        
        
