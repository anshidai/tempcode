<div class="go-top topdn" id="go-top"><a href="javascript:;" class="go"></a></div>
<script type="text/javascript">
    jQuery(function(){
    
        //select-color 
        jQuery(".change-color-btn").click(function(){
            jQuery(".select-color-block").toggle('slow');
        });
        
        jQuery(".select-color-block .close").click(function(){
            jQuery(".select-color-block").toggle('slow').hide();
        });
    
        jQuery(".show-color dd").click(function(){
            var _this = jQuery(this);
            var imgObj = _this.find("img");
            jQuery(".show-color dd").find("i").removeClass('curr');
            _this.find("i").addClass('curr');

            jQuery("#default-clorimg").attr("src", imgObj.attr('src'));
            jQuery(".color-tit").html(imgObj.attr('title'));
            jQuery(".color-enable i").addClass("curr");
            jQuery(".color-enable img").attr("src", imgObj.attr('src'));
            jQuery("#attrColor").val(imgObj.attr('title'));
        });
        
        jQuery(".show-color dd").hover(function(){
            var _this = jQuery(this);
            var imgObj = _this.find("img");
            jQuery("#default-clorimg").attr("src", imgObj.attr('src'));
            jQuery(".color-tit").html(imgObj.attr('title'));
        });
        //select-color end
    
    
        showScroll();
        function showScroll(){
            jQuery(window).scroll( function() { 
                var scrollValue=jQuery(window).scrollTop();
                scrollValue > 100 ? jQuery('.go-top').fadeIn():jQuery('.go-top').fadeOut();
            } );    
            jQuery('#go-top').click(function(){
                jQuery("html,body").animate({scrollTop:0},200);    
            });    
        }
    })
</script>
